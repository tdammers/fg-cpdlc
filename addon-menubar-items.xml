<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <meta>
        <file-type type="string">FlightGear add-on menu bar items</file-type>
        <format-version type="int">1</format-version>
    </meta>
    <menubar-items>
        <menu>
            <label>CPDLC</label>
            <enabled type="bool">true</enabled>
            
            <item>
                <label>CPDLC</label>
                <binding>
                    <command>dialog-show</command>
                    <dialog-name>addon-cpdlc-dialog</dialog-name>
                </binding>
            </item>
            
            <item>
                <label>Reload CPDLC Addon</label>
                <binding>
                    <command>addon-reload</command>
                    <id>nl.tobiasdammers.fg-cpdlc</id>
                </binding>
            </item> 
            
        </menu>
    </menubar-items>
</PropertyList>
